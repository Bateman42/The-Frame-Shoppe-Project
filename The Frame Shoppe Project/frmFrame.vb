Public Class frmFrame

    Private Sub frameBasketWeave_Click(sender As Object, e As EventArgs) Handles frameBasketWeave.Click
        'Set frame price
        frmMain.dblPriceFrame = dblFrame1

        ' Display the name frame1
        frmMain.lblFrameName.Text = strFrame1Name.ToString()

        'Update view
        frmMain.updateField()

        'Close frmFrame
        Me.Close()
    End Sub

